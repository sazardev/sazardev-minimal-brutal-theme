  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#b5b5b5"
      }
    },
    {
      "name": "Keyword, Storage - Armonía con verde",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "keyword.operator.new",
        "keyword.other.special-method",
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.default",
        "keyword.control.go"
      ],
      "settings": {
        "foreground": "#0066cc",
        "fontStyle": ""
      }
    },
    {
      "name": "String - Verde robusto",
      "scope": [
        "string",
        "string.quoted",
        "string.template",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#009944"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "constant.escape"
      ],
      "settings": {
        "foreground": "#cc8800"
      }
    },
    {
      "name": "Function - Verde NEÓN Principal",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "entity.name.method"
      ],
      "settings": {
        "foreground": "#00ff88",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Class, Type",
      "scope": [
        "entity.name.class",
        "entity.name.type",
        "support.type",
        "support.class",
        "entity.name.interface"
      ],
      "settings": {
        "foreground": "#0088aa"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable",
        "variable.parameter",
        "variable.other",
        "variable.language.this",
        "variable.language.super"
      ],
      "settings": {
        "foreground": "#1a1a1a"
      }
    },
    {
      "name": "Property - Azul vibrante para p.repo",
      "scope": [
        "variable.object.property",
        "meta.object-literal.key",
        "support.variable.property",
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css",
        "meta.field.declaration.go",
        "variable.field.go"
      ],
      "settings": {
        "foreground": "#0077cc"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#aa5577"
      }
    },
    {
      "name": "Attribute",
      "scope": [
        "entity.other.attribute-name",
        "entity.other.attribute-name.html"
      ],
      "settings": {
        "foreground": "#cc8800"
      }
    },
    {
      "name": "Operator - Coherente con verde neón",
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "keyword.operator.logical",
        "keyword.operator.comparison.go",
        "punctuation.separator.key-value"
      ],
      "settings": {
        "foreground": "#55cc55"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#8f8f8f"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant",
        "constant.other",
        "support.constant"
      ],
      "settings": {
        "foreground": "#7744aa"
      }
    },
    {
      "name": "Regular Expression - Verde NEÓN",
      "scope": [
        "string.regexp",
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#00ff88"
      }
    },
    {
      "name": "Import/Module",
      "scope": [
        "entity.name.import",
        "entity.name.module",
        "support.module"
      ],
      "settings": {
        "foreground": "#5577cc"
      }
    },
    {
      "name": "Decorator",
      "scope": [
        "punctuation.decorator"
      ],
      "settings": {
        "foreground": "#cc6600"
      }
    },
    {
      "name": "Escape Characters - Verde NEÓN",
      "scope": [
        "constant.character.entity"
      ],
      "settings": {
        "foreground": "#00ff88"
      }
    },
    {
      "name": "Template Expression - Verde NEÓN",
      "scope": [
        "punctuation.definition.template-expression"
      ],
      "settings": {
        "foreground": "#00ff88"
      }
    },
    {
      "name": "Go - Package y struct fields más vivos",
      "scope": [
        "meta.struct.go variable.other.go"
      ],
      "settings": {
        "foreground": "#0077cc"
      }
    },
    {
      "name": "Go - Métodos receiver más visible",
      "scope": [
        "variable.parameter.receiver.go"
      ],
      "settings": {
        "foreground": "#0088dd"
      }
    },
    {
      "name": "Go - Interfaces y tipos",
      "scope": [
