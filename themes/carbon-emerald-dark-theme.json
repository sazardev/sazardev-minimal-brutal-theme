  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#4a4a4a"
      }
    },
    {
      "name": "Keyword, Storage - Armonía con verde",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "keyword.operator.new",
        "keyword.other.special-method",
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.default",
        "keyword.control.go"
      ],
      "settings": {
        "foreground": "#66ccff",
        "fontStyle": ""
      }
    },
    {
      "name": "String - Sutil pero visible",
      "scope": [
        "string",
        "string.quoted",
        "string.template",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#99ddaa"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "constant.escape"
      ],
      "settings": {
        "foreground": "#c4b078"
      }
    },
    {
      "name": "Function - Verde NEON",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "entity.name.method"
      ],
      "settings": {
        "foreground": "#00ff88",
        "fontStyle": ""
      }
    },
    {
      "name": "Class, Type",
      "scope": [
        "entity.name.class",
        "entity.name.type",
        "support.type",
        "support.class",
        "entity.name.interface"
      ],
      "settings": {
        "foreground": "#7cb3b3"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable",
        "variable.parameter",
        "variable.other",
        "variable.language.this",
        "variable.language.super"
      ],
      "settings": {
        "foreground": "#e8e8e8"
      }
    },
    {
      "name": "Property - Más visible para Go",
      "scope": [
        "variable.object.property",
        "meta.object-literal.key",
        "support.variable.property",
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css",
        "meta.field.declaration.go",
        "variable.field.go"
