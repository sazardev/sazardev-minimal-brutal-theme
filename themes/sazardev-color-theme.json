{
  "name": "sazardev",
  "type": "dark",
  "colors": {
    // === EDITOR CORE - Ultra Clean ===
    "editor.background": "#0a0b0a",
    "editor.foreground": "#f5f5f5",
    "editorCursor.foreground": "#00ff88",
    "editor.selectionBackground": "#00ff8822",
    "editor.selectionHighlightBackground": "#00ff8811",
    "editor.lineHighlightBackground": "#00000000",
    "editorLineNumber.foreground": "#333333",
    "editorLineNumber.activeForeground": "#00ff88",
    "editorIndentGuide.background1": "#1a1a1a",
    "editorIndentGuide.activeBackground1": "#00ff88",
    "editorRuler.foreground": "#1a1a1a",
    "editorWhitespace.foreground": "#222222",
    "editor.wordHighlightBackground": "#00ff8811",
    "editor.wordHighlightStrongBackground": "#00ff8822",
    "editor.findMatchBackground": "#0088ff33",
    "editor.findMatchHighlightBackground": "#0088ff22",
    // === WORKBENCH - Minimal & Clean ===
    "activityBar.background": "#060706",
    "activityBar.foreground": "#ffffff",
    "activityBar.inactiveForeground": "#666666",
    "activityBarBadge.background": "#00ff88",
    "activityBarBadge.foreground": "#000000",
    "activityBar.border": "#00000000",
    "activityBar.activeBorder": "#00ff88",
    "activityBar.activeBackground": "#00000000",
    "sideBar.background": "#080908",
    "sideBar.foreground": "#cccccc",
    "sideBar.border": "#00000000",
    "sideBarTitle.foreground": "#ffffff",
    "sideBarSectionHeader.background": "#00000000",
    "sideBarSectionHeader.foreground": "#ffffff",
    "sideBarSectionHeader.border": "#00000000",
    "titleBar.activeBackground": "#060706",
    "titleBar.activeForeground": "#ffffff",
    "titleBar.inactiveBackground": "#060706",
    "titleBar.inactiveForeground": "#666666",
    "titleBar.border": "#00000000",
    "statusBar.background": "#060706",
    "statusBar.foreground": "#ffffff",
    "statusBar.border": "#00ff8844",
    "statusBar.debuggingBackground": "#ff0055",
    "statusBar.debuggingForeground": "#ffffff",
    "statusBar.noFolderBackground": "#000000",
    // === TABS - Brutal Minimal Design ===
    "tab.activeBackground": "#00000000",
    "tab.activeForeground": "#ffffff",
    "tab.activeBorder": "#00000000",
    "tab.activeBorderTop": "#00ff88",
    "tab.inactiveBackground": "#00000000",
    "tab.inactiveForeground": "#666666",
    "tab.border": "#00000000",
    "tab.hoverBackground": "#0a0a0a44",
    "tab.hoverForeground": "#cccccc",
    "tab.hoverBorder": "#00000000",
    "tab.unfocusedActiveForeground": "#cccccc",
    "tab.unfocusedInactiveForeground": "#555555",
    "tab.lastPinnedBorder": "#00000000",
    "editorGroupHeader.tabsBackground": "#000000",
    "editorGroupHeader.tabsBorder": "#00000000",
    "editorGroupHeader.border": "#00000000",
    // === PANEL - Clean & Focused ===
    "panel.background": "#050505",
    "panel.border": "#00000000",
    "panelTitle.activeForeground": "#ffffff",
    "panelTitle.inactiveForeground": "#666666",
    "panelTitle.activeBorder": "#00ff88",
    "panelSectionHeader.background": "#00000000",
    "panelSectionHeader.foreground": "#ffffff",
    "panelSectionHeader.border": "#00000000",
    // === TERMINAL ===
    "terminal.background": "#000000",
    "terminal.foreground": "#f5f5f5",
    "terminal.ansiBlack": "#000000",
    "terminal.ansiRed": "#ff0055",
    "terminal.ansiGreen": "#00ff88",
    "terminal.ansiYellow": "#ffdd00",
    "terminal.ansiBlue": "#0088ff",
    "terminal.ansiMagenta": "#ff00dd",
    "terminal.ansiCyan": "#00ffdd",
    "terminal.ansiWhite": "#ffffff",
    "terminal.ansiBrightBlack": "#333333",
    "terminal.ansiBrightRed": "#ff3377",
    "terminal.ansiBrightGreen": "#33ffaa",
    "terminal.ansiBrightYellow": "#ffee33",
    "terminal.ansiBrightBlue": "#3399ff",
    "terminal.ansiBrightMagenta": "#ff33ee",
    "terminal.ansiBrightCyan": "#33ffee",
    "terminal.ansiBrightWhite": "#ffffff",
    // === BUTTONS & INPUTS ===
    "button.background": "#00ff88",
    "button.foreground": "#000000",
    "button.hoverBackground": "#33ff99",
    "input.background": "#0a0a0a",
    "input.foreground": "#ffffff",
    "input.border": "#333333",
    "input.placeholderForeground": "#666666",
    "inputOption.activeBorder": "#00ff88",
    "inputValidation.errorBackground": "#ff0055",
    "inputValidation.errorBorder": "#ff0055",
    // === LISTS - Minimal Selection ===
    "list.activeSelectionBackground": "#00ff8822",
    "list.activeSelectionForeground": "#ffffff",
    "list.inactiveSelectionBackground": "#00ff8811",
    "list.inactiveSelectionForeground": "#cccccc",
    "list.hoverBackground": "#0088ff11",
    "list.hoverForeground": "#ffffff",
    "list.focusBackground": "#00ff8822",
    "list.focusForeground": "#ffffff",
    "list.dropBackground": "#00ff8833",
    "list.highlightForeground": "#00ff88",
    // === SCROLLBAR ===
    "scrollbar.shadow": "#000000",
    "scrollbarSlider.background": "#333333",
    "scrollbarSlider.hoverBackground": "#444444",
    "scrollbarSlider.activeBackground": "#555555",
    // === MENU ===
    "menu.background": "#000000",
    "menu.foreground": "#ffffff",
    "menu.selectionBackground": "#1a1a1a",
    "menu.selectionForeground": "#ffffff",
    "menu.border": "#333333",
    // === NOTIFICATIONS ===
    "notificationCenter.border": "#333333",
    "notificationCenterHeader.background": "#0a0a0a",
    "notificationToast.border": "#333333",
    "notifications.background": "#0a0a0a",
    "notifications.foreground": "#ffffff",
    "notifications.border": "#333333",
    // === BREADCRUMBS ===
    "breadcrumb.background": "#0a0a0a",
    "breadcrumb.foreground": "#cccccc",
    "breadcrumb.focusForeground": "#ffffff",
    "breadcrumb.activeSelectionForeground": "#00ff88",
    // === PEEK VIEW ===
    "peekView.border": "#00ff88",
    "peekViewEditor.background": "#0a0a0a",
    "peekViewResult.background": "#050505",
    "peekViewTitle.background": "#000000",
    // === DIFF EDITOR ===
    "diffEditor.insertedTextBackground": "#00ff8822",
    "diffEditor.removedTextBackground": "#ff005522",
    "diffEditor.insertedLineBackground": "#00ff8811",
    "diffEditor.removedLineBackground": "#ff005511",
    // === GIT DECORATIONS ===
    "gitDecoration.addedResourceForeground": "#00ff88",
    "gitDecoration.modifiedResourceForeground": "#ffdd00",
    "gitDecoration.deletedResourceForeground": "#ff0055",
    "gitDecoration.untrackedResourceForeground": "#0088ff",
    "gitDecoration.ignoredResourceForeground": "#666666",
    "gitDecoration.conflictingResourceForeground": "#ff00dd",
    // === MINIMAP ===
    "minimap.background": "#000000",
    "minimap.selectionHighlight": "#00ff8844",
    "minimap.errorHighlight": "#ff0055",
    "minimap.warningHighlight": "#ffdd00",
    "minimap.findMatchHighlight": "#0088ff44",
    // === WIDGET ===
    "widget.shadow": "#000000",
    "editorWidget.background": "#0a0a0a",
    "editorWidget.border": "#333333",
    "editorHoverWidget.background": "#0a0a0a",
    "editorHoverWidget.border": "#333333",
    // === PROGRESS BAR ===
    "progressBar.background": "#00ff88",
    // === FOCUS BORDER ===
    "focusBorder": "#00ff88",
    // === CONTRAST BORDERS ===
    "contrastBorder": "#333333",
    "contrastActiveBorder": "#00ff88"
  },
  "tokenColors": [
    // === COMMENTS - Subtle but present ===
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#555555"
      }
    },
    // === KEYWORDS - Electric blue for maximum impact ===
    {
      "name": "Keyword, Storage",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "keyword.operator",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#0088ff",
        "fontStyle": "bold"
      }
    },
    // === STRINGS - Bright green for visibility ===
    {
      "name": "String",
      "scope": ["string", "string.quoted", "string.template"],
      "settings": {
        "foreground": "#00ff88"
      }
    },
    // === NUMBERS - Electric yellow ===
    {
      "name": "Number",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape"
      ],
      "settings": {
        "foreground": "#ffdd00"
      }
    },
    // === FUNCTIONS - Bright white for clarity ===
    {
      "name": "Function",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#ffffff",
        "fontStyle": "bold"
      }
    },
    // === CLASSES & TYPES - Cyan for structure ===
    {
      "name": "Class, Type",
      "scope": [
        "entity.name.class",
        "entity.name.type",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#00ffdd",
        "fontStyle": "bold"
      }
    },
    // === VARIABLES - Clean white ===
    {
      "name": "Variable",
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#f5f5f5"
      }
    },
    // === OPERATORS - Bright cyan ===
    {
      "name": "Operator",
      "scope": [
        "keyword.operator",
        "punctuation.definition.tag",
        "punctuation.separator",
        "punctuation.definition",
        "meta.brace",
        "punctuation.section"
      ],
      "settings": {
        "foreground": "#00ffdd"
      }
    },
    // === TAGS - Hot pink for HTML/XML ===
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#ff00dd",
        "fontStyle": "bold"
      }
    },
    // === ATTRIBUTES - Soft pink ===
    {
      "name": "Attribute",
      "scope": [
        "entity.other.attribute-name",
        "entity.other.attribute-name.html"
      ],
      "settings": {
        "foreground": "#ff88dd"
      }
    },
    // === INVALID - Aggressive red ===
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#ff0055",
        "fontStyle": "bold"
      }
    },
    // === CONSTANTS - Orange ===
    {
      "name": "Constant",
      "scope": ["constant.other", "variable.parameter"],
      "settings": {
        "foreground": "#ff8800"
      }
    },
    // === REGEX - Bright magenta ===
    {
      "name": "Regular Expression",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#ff00dd"
      }
    },
    // === ESCAPE CHARACTERS - Neon green ===
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#88ff00"
      }
    },
    // === URL - Underlined cyan ===
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "foreground": "#00ffdd",
        "fontStyle": "underline"
      }
    },
    // === JSON KEYS - Hierarchical colors ===
    {
      "name": "JSON Key - Level 0",
      "scope": [
        "source.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "JSON Key - Level 1",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#00ffdd"
      }
    },
    {
      "name": "JSON Key - Level 2",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#0088ff"
      }
    },
    // === MARKDOWN ===
    {
      "name": "Markdown - Heading",
      "scope": ["markup.heading", "punctuation.definition.heading.markdown"],
      "settings": {
        "foreground": "#ffffff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown - Bold",
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#ffffff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown - Italic",
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#cccccc",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown - Code",
      "scope": ["markup.inline.raw", "markup.raw.block"],
      "settings": {
        "foreground": "#00ff88"
      }
    },
    {
      "name": "Markdown - Link",
      "scope": [
        "string.other.link.title.markdown",
        "constant.other.reference.link.markdown"
      ],
      "settings": {
        "foreground": "#0088ff"
      }
    },
    // === CSS ===
    {
      "name": "CSS Property",
      "scope": ["support.type.property-name.css"],
      "settings": {
        "foreground": "#00ffdd"
      }
    },
    {
      "name": "CSS Value",
      "scope": ["support.constant.property-value.css"],
      "settings": {
        "foreground": "#ffdd00"
      }
    },
    {
      "name": "CSS Selector",
      "scope": [
        "entity.name.tag.css",
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#ff00dd"
      }
    },
    // === TYPESCRIPT/JAVASCRIPT ===
    {
      "name": "TypeScript/JavaScript - Interface",
      "scope": ["entity.name.type.interface"],
      "settings": {
        "foreground": "#00ffdd",
        "fontStyle": "bold"
      }
    },
    {
      "name": "TypeScript/JavaScript - Decorator",
      "scope": ["meta.decorator", "punctuation.decorator"],
      "settings": {
        "foreground": "#ff8800"
      }
    },
    // === PYTHON ===
    {
      "name": "Python - Decorator",
      "scope": [
        "meta.function.decorator.python",
        "punctuation.definition.decorator.python"
      ],
      "settings": {
        "foreground": "#ff8800"
      }
    },
    {
      "name": "Python - Self",
      "scope": ["variable.language.special.self.python"],
      "settings": {
        "foreground": "#ff00dd",
        "fontStyle": "italic"
      }
    },
    // === GIT GUTTER ===
    {
      "name": "diff.header",
      "scope": ["meta.diff", "meta.diff.header"],
      "settings": {
        "foreground": "#666666"
      }
    },
    {
      "name": "diff.deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#ff0055"
      }
    },
    {
      "name": "diff.inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#00ff88"
      }
    },
    {
      "name": "diff.changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#ffdd00"
      }
    }
  ]
}
