{
  "name": "Carbon Code Crimson Light",
  "type": "light",
  "colors": {
    "editor.background": "#fefcfc",
    "editor.foreground": "#0a0a0a",
    "editorCursor.foreground": "#dc2626",
    "editor.selectionBackground": "#dc262644",
    "editor.selectionHighlightBackground": "#dc262633",
    "editor.lineHighlightBackground": "#00000000",
    "editor.wordHighlightBackground": "#dc262622",
    "editor.wordHighlightStrongBackground": "#dc262633",
    "editor.findMatchBackground": "#f59e0b55",
    "editor.findMatchHighlightBackground": "#f59e0b33",
    "editor.rangeHighlightBackground": "#dc262622",
    "editorLineNumber.foreground": "#999999",
    "editorLineNumber.activeForeground": "#dc2626",
    "editorIndentGuide.background1": "#dddddd",
    "editorIndentGuide.activeBackground1": "#dc2626",
    "editorRuler.foreground": "#dddddd",
    "editorWhitespace.foreground": "#cccccc",
    "activityBar.background": "#fcf7f7",
    "activityBar.foreground": "#1a1a1a",
    "activityBar.inactiveForeground": "#777777",
    "activityBarBadge.background": "#dc2626",
    "activityBarBadge.foreground": "#ffffff",
    "activityBar.border": "#00000000",
    "activityBar.activeBorder": "#dc2626",
    "activityBar.activeBackground": "#00000000",
    "activityBar.activeFocusBorder": "#00000000",
    "activityBar.dropBorder": "#00000000",
    "sideBar.background": "#f2f2f2",
    "sideBar.foreground": "#333333",
    "sideBar.border": "#00000000",
    "sideBarTitle.foreground": "#1a1a1a",
    "sideBarSectionHeader.background": "#00000000",
    "sideBarSectionHeader.foreground": "#1a1a1a",
    "sideBarSectionHeader.border": "#00000000",
    "titleBar.activeBackground": "#f8f8f8",
    "titleBar.activeForeground": "#1a1a1a",
    "titleBar.inactiveBackground": "#f8f8f8",
    "titleBar.inactiveForeground": "#777777",
    "titleBar.border": "#00000000",
    "statusBar.background": "#f8f8f8",
    "statusBar.foreground": "#333333",
    "statusBar.border": "#dc262655",
    "statusBar.debuggingBackground": "#dc2626",
    "statusBar.debuggingForeground": "#ffffff",
    "statusBar.noFolderBackground": "#f8f8f8",
    "tab.activeBackground": "#00000000",
    "tab.activeForeground": "#1a1a1a",
    "tab.activeBorder": "#00000000",
    "tab.activeBorderTop": "#dc2626",
    "tab.inactiveBackground": "#00000000",
    "tab.inactiveForeground": "#777777",
    "tab.border": "#00000000",
    "tab.hoverBackground": "#eeeeee44",
    "tab.hoverForeground": "#333333",
    "tab.hoverBorder": "#00000000",
    "tab.unfocusedActiveForeground": "#444444",
    "tab.unfocusedInactiveForeground": "#888888",
    "tab.lastPinnedBorder": "#00000000",
    "tab.focusBorder": "#00000000",
    "tab.unfocusedActiveBorder": "#00000000",
    "tab.unfocusedActiveBorderTop": "#00000000",
    "editorGroupHeader.tabsBackground": "#f8f8f8",
    "editorGroupHeader.tabsBorder": "#00000000",
    "editorGroupHeader.border": "#00000000",
    "panel.background": "#fdfdfd",
    "panel.border": "#cccccc",
    "panelTitle.activeForeground": "#000000",
    "panelTitle.inactiveForeground": "#777777",
    "panelTitle.activeBorder": "#dc2626",
    "terminal.background": "#ffffff",
    "terminal.foreground": "#000000",
    "terminal.ansiBlack": "#000000",
    "terminal.ansiRed": "#dc2626",
    "terminal.ansiGreen": "#16a34a",
    "terminal.ansiYellow": "#f59e0b",
    "terminal.ansiBlue": "#2563eb",
    "terminal.ansiMagenta": "#c026d3",
    "terminal.ansiCyan": "#0891b2",
    "terminal.ansiWhite": "#ffffff",
    "button.background": "#dc2626",
    "button.foreground": "#ffffff",
    "button.hoverBackground": "#b91c1c",
    "input.background": "#ffffff",
    "input.foreground": "#000000",
    "input.border": "#cccccc",
    "inputOption.activeBorder": "#dc2626",
    "list.activeSelectionBackground": "#dc262622",
    "list.activeSelectionForeground": "#000000",
    "list.dropBackground": "#dc262633",
    "list.inactiveSelectionBackground": "#dc262622",
    "list.hoverBackground": "#dc262615",
    "list.hoverForeground": "#000000",
    "list.inactiveSelectionForeground": "#000000",
    "list.focusBackground": "#dc262633",
    "list.focusForeground": "#000000",
    "list.focusHighlightForeground": "#dc2626",
    "focusBorder": "#00000000",
    "contrastBorder": "#cccccc",
    "contrastActiveBorder": "#dc2626",
    "tree.indentGuidesStroke": "#dc262644",
    "tree.inactiveIndentGuidesStroke": "#dc262644"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#666666"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#0a0a0a"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.require",
        "support.function.any-method"
      ],
      "settings": {
        "foreground": "#dc2626"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.class",
        "entity.name.type.class"
      ],
      "settings": {
        "foreground": "#f59e0b"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#c026d3"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#16a34a"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#2563eb"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape"
      ],
      "settings": {
        "foreground": "#0891b2"
      }
    },
    {
      "name": "Dart Class and Types",
      "scope": [
        "support.class.dart",
        "support.type.dart",
        "entity.name.type.class.dart",
        "entity.name.type.dart"
      ],
      "settings": {
        "foreground": "#7cb3b3"
      }
    },
    {
      "name": "Dart Built-in Types",
      "scope": [
        "storage.type.primitive.dart",
        "storage.type.built-in.dart"
      ],
      "settings": {
        "foreground": "#66ccff"
      }
    },
    {
      "name": "Dart Functions",
      "scope": [
        "entity.name.function.dart",
        "support.function.dart"
      ],
      "settings": {
        "foreground": "#6366f1"
      }
    },
    {
      "name": "Dart Variables and Properties",
      "scope": [
        "variable.other.dart",
        "variable.parameter.dart",
        "variable.declaration.dart",
        "meta.definition.variable.dart"
      ],
      "settings": {
        "foreground": "#e8e8e8"
      }
    },
    {
      "name": "Dart Property Access",
      "scope": [
        "variable.other.property.dart",
        "meta.field.declaration.dart"
      ],
      "settings": {
        "foreground": "#a8d0ff"
      }
    },
    {
      "name": "Dart Keywords",
      "scope": [
        "keyword.control.dart",
        "keyword.declaration.dart",
        "storage.type.annotation.dart"
      ],
      "settings": {
        "foreground": "#66ccff"
      }
    },
    {
      "name": "Flutter Widgets",
      "scope": [
        "support.class.flutter",
        "support.type.flutter",
        "meta.function-call.dart entity.name.function.dart"
      ],
      "settings": {
        "foreground": "#dd88ff"
      }
    },
    {
      "name": "Dart String Interpolation",
      "scope": [
        "string.interpolated.dart",
        "punctuation.definition.interpolation.dart",
        "meta.template.expression.dart"
      ],
      "settings": {
        "foreground": "#6366f1"
      }
    },
    {
      "name": "Dart Annotations",
      "scope": [
        "storage.type.annotation.dart",
        "meta.declaration.annotation.dart"
      ],
      "settings": {
        "foreground": "#a87cb3"
      }
    },
    {
      "name": "Dart Constructor",
      "scope": [
        "entity.name.constructor",
        "meta.constructor.dart"
      ],
      "settings": {
        "foreground": "#7cb3b3"
      }
    },
    {
      "name": "Rust Types",
      "scope": [
        "entity.name.type.rust",
        "storage.type.rust",
        "storage.class.std.rust"
      ],
      "settings": {
        "foreground": "#7cb3b3"
      }
    },
    {
      "name": "Rust Macros",
      "scope": [
        "meta.macro.rust",
        "entity.name.function.macro.rust"
      ],
      "settings": {
        "foreground": "#a87cb3"
      }
    },
    {
      "name": "Swift Types",
      "scope": [
        "entity.name.type.swift",
        "support.type.swift"
      ],
      "settings": {
        "foreground": "#7cb3b3"
      }
    },
    {
      "name": "PHP Special Variables",
      "scope": [
        "variable.language.php",
        "variable.other.global.php"
      ],
      "settings": {
        "foreground": "#dd88ff"
      }
    },
    {
      "name": "Ruby Symbols",
      "scope": [
        "constant.other.symbol.ruby"
      ],
      "settings": {
        "foreground": "#c4b078"
      }
    },
    {
      "name": "C# Types",
      "scope": [
        "entity.name.type.class.cs",
        "storage.type.cs"
      ],
      "settings": {
        "foreground": "#7cb3b3"
      }
    },
    {
      "name": "C# Attributes",
      "scope": [
        "entity.name.tag.cs",
        "meta.attribute.cs"
      ],
      "settings": {
        "foreground": "#a87cb3"
      }
    },
    {
      "name": "TypeScript Decorators",
      "scope": [
        "meta.decorator.ts",
        "meta.decorator.tsx"
      ],
      "settings": {
        "foreground": "#a87cb3"
      }
    },
    {
      "name": "React/JSX Tags",
      "scope": [
        "entity.name.tag.js",
        "entity.name.tag.tsx",
        "entity.name.tag.jsx",
        "support.class.component.js",
        "support.class.component.tsx"
      ],
      "settings": {
        "foreground": "#dd88ff"
      }
    },
    {
      "name": "JSX/TSX Component Tags",
      "scope": [
        "entity.name.tag.js.jsx",
        "entity.name.tag.tsx"
      ],
      "settings": {
        "foreground": "#dd88ff"
      }
    },
    {
      "name": "HTML Tags",
      "scope": [
        "entity.name.tag.html",
        "entity.name.tag.inline.any.html",
        "entity.name.tag.other.html",
        "entity.name.tag.block.any.html"
      ],
      "settings": {
        "foreground": "#7aa3c4"
      }
    },
    {
      "name": "YAML Keys",
      "scope": [
        "entity.name.tag.yaml"
      ],
      "settings": {
        "foreground": "#7cb3b3"
      }
    },
    {
      "name": "Java Types",
      "scope": [
        "entity.name.type.class.java",
        "storage.type.java",
        "storage.type.generic.java"
      ],
      "settings": {
        "foreground": "#7cb3b3"
      }
    },
    {
      "name": "Kotlin Types",
      "scope": [
        "entity.name.type.class.kotlin",
        "storage.type.kotlin"
      ],
      "settings": {
        "foreground": "#7cb3b3"
      }
    }
  ]
}
