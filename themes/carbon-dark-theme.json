{
    "name": "Carbon Code Dark",
    "type": "dark",
    "colors": {
        // === EDITOR CORE - Professional & Clean ===
        "editor.background": "#0f0f0f",
        "editor.foreground": "#e6e6e6",
        "editorCursor.foreground": "#6366f1",
        "editor.selectionBackground": "#6366f133",
        "editor.selectionHighlightBackground": "#6366f122",
        "editor.lineHighlightBackground": "#00000000",
        "editor.wordHighlightBackground": "#6366f115",
        "editor.wordHighlightStrongBackground": "#6366f125",
        "editor.findMatchBackground": "#0ea5e944",
        "editor.findMatchHighlightBackground": "#0ea5e922",
        "editor.rangeHighlightBackground": "#6366f111",
        "editorLineNumber.foreground": "#454545",
        "editorLineNumber.activeForeground": "#6366f1",
        "editorIndentGuide.background1": "#1c1c1c",
        "editorIndentGuide.activeBackground1": "#6366f1",
        "editorRuler.foreground": "#1c1c1c",
        "editorWhitespace.foreground": "#2a2a2a",
        // === WORKBENCH - Clean & Elegant ===
        "activityBar.background": "#080808",
        "activityBar.foreground": "#d0d0d0",
        "activityBar.inactiveForeground": "#606060",
        "activityBarBadge.background": "#6366f1",
        "activityBarBadge.foreground": "#ffffff",
        "activityBar.border": "#00000000",
        "activityBar.activeBorder": "#6366f1",
        "activityBar.activeBackground": "#00000000",
        "activityBar.activeFocusBorder": "#00000000",
        "activityBar.dropBorder": "#00000000",
        "sideBar.background": "#0a0a0a",
        "sideBar.foreground": "#b8b8b8",
        "sideBar.border": "#00000000",
        "sideBarTitle.foreground": "#d0d0d0",
        "sideBarSectionHeader.background": "#00000000",
        "sideBarSectionHeader.foreground": "#d0d0d0",
        "sideBarSectionHeader.border": "#00000000",
        "titleBar.activeBackground": "#080808",
        "titleBar.activeForeground": "#d0d0d0",
        "titleBar.inactiveBackground": "#080808",
        "titleBar.inactiveForeground": "#606060",
        "titleBar.border": "#00000000",
        "statusBar.background": "#080808",
        "statusBar.foreground": "#b8b8b8",
        "statusBar.border": "#6366f133",
        "statusBar.debuggingBackground": "#c85450",
        "statusBar.debuggingForeground": "#ffffff",
        "statusBar.noFolderBackground": "#080808",
        // === TABS - Brutal Minimal Design ===
        "tab.activeBackground": "#00000000",
        "tab.activeForeground": "#d0d0d0",
        "tab.activeBorder": "#00000000",
        "tab.activeBorderTop": "#6366f1",
        "tab.inactiveBackground": "#00000000",
        "tab.inactiveForeground": "#707070",
        "tab.border": "#00000000",
        "tab.hoverBackground": "#0f0f0f22",
        "tab.hoverForeground": "#b8b8b8",
        "tab.hoverBorder": "#00000000",
        "tab.unfocusedActiveForeground": "#a0a0a0",
        "tab.unfocusedInactiveForeground": "#606060",
        "tab.lastPinnedBorder": "#00000000",
        "tab.focusBorder": "#00000000",
        "tab.unfocusedActiveBorder": "#00000000",
        "tab.unfocusedActiveBorderTop": "#00000000",
        "editorGroupHeader.tabsBackground": "#080808",
        "editorGroupHeader.tabsBorder": "#00000000",
        "editorGroupHeader.border": "#00000000",
        // === PANEL ===
        "panel.background": "#050505",
        "panel.border": "#1a1a1a",
        "panelTitle.activeForeground": "#ffffff",
        "panelTitle.inactiveForeground": "#666666",
        "panelTitle.activeBorder": "#6366f1",
        // === TERMINAL ===
        "terminal.background": "#000000",
        "terminal.foreground": "#f5f5f5",
        "terminal.ansiBlack": "#000000",
        "terminal.ansiRed": "#ff0055",
        "terminal.ansiGreen": "#6366f1",
        "terminal.ansiYellow": "#ffdd00",
        "terminal.ansiBlue": "#0088ff",
        "terminal.ansiMagenta": "#ff00dd",
        "terminal.ansiCyan": "#00ffdd",
        "terminal.ansiWhite": "#ffffff",
        "terminal.ansiBrightBlack": "#333333",
        "terminal.ansiBrightRed": "#ff3377",
        "terminal.ansiBrightGreen": "#33ffaa",
        "terminal.ansiBrightYellow": "#ffee33",
        "terminal.ansiBrightBlue": "#3399ff",
        "terminal.ansiBrightMagenta": "#ff33ee",
        "terminal.ansiBrightCyan": "#33ffee",
        "terminal.ansiBrightWhite": "#ffffff",
        // === BUTTONS & INPUTS ===
        "button.background": "#6366f1",
        "button.foreground": "#000000",
        "button.hoverBackground": "#33ff99",
        "input.background": "#0a0a0a",
        "input.foreground": "#ffffff",
        "input.border": "#333333",
        "input.placeholderForeground": "#666666",
        "inputOption.activeBorder": "#6366f1",
        "inputValidation.errorBackground": "#ff0055",
        "inputValidation.errorBorder": "#ff0055",
        // === LISTS ===
        "list.activeSelectionBackground": "#6366f111",
        "list.activeSelectionForeground": "#ffffff",
        "list.inactiveSelectionForeground": "#cccccc",
        "list.inactiveSelectionBackground": "#6366f111",
        "list.hoverBackground": "#6366f115",
        "list.hoverForeground": "#ffffff",
        "list.focusBackground": "#1a1a1a",
        "list.focusForeground": "#ffffff",
        "list.focusHighlightForeground": "#6366f1",
        "list.focusOutline": "#00000000",
        "list.focusAndSelectionOutline": "#00000000",
        "list.activeSelectionIconForeground": "#ffffff",
        "list.inactiveSelectionIconForeground": "#cccccc",
        // === SCROLLBAR ===
        "scrollbar.shadow": "#000000",
        "scrollbarSlider.background": "#333333",
        "scrollbarSlider.hoverBackground": "#444444",
        "scrollbarSlider.activeBackground": "#555555",
        // === MENU ===
        "menu.background": "#000000",
        "menu.foreground": "#ffffff",
        "menu.selectionBackground": "#1a1a1a",
        "menu.selectionForeground": "#ffffff",
        "menu.border": "#333333",
        // === NOTIFICATIONS ===
        "notificationCenter.border": "#333333",
        "notificationCenterHeader.background": "#0a0a0a",
        "notificationToast.border": "#333333",
        "notifications.background": "#0a0a0a",
        "notifications.foreground": "#ffffff",
        "notifications.border": "#333333",
        // === BREADCRUMBS ===
        "breadcrumb.background": "#0a0a0a",
        "breadcrumb.foreground": "#cccccc",
        "breadcrumb.focusForeground": "#ffffff",
        "breadcrumb.activeSelectionForeground": "#6366f1",
        // === PEEK VIEW ===
        "peekView.border": "#6366f1",
        "peekViewEditor.background": "#0a0a0a",
        "peekViewResult.background": "#050505",
        "peekViewTitle.background": "#000000",
        // === DIFF EDITOR ===
        "diffEditor.insertedTextBackground": "#6366f122",
        "diffEditor.removedTextBackground": "#ff005522",
        "diffEditor.insertedLineBackground": "#6366f111",
        "diffEditor.removedLineBackground": "#ff005511",
        // === GIT DECORATIONS ===
        "gitDecoration.addedResourceForeground": "#6366f1",
        "gitDecoration.modifiedResourceForeground": "#ffdd00",
        "gitDecoration.deletedResourceForeground": "#ff0055",
        "gitDecoration.untrackedResourceForeground": "#0088ff",
        "gitDecoration.ignoredResourceForeground": "#666666",
        "gitDecoration.conflictingResourceForeground": "#ff00dd",
        // === MINIMAP ===
        "minimap.background": "#000000",
        "minimap.selectionHighlight": "#6366f144",
        "minimap.errorHighlight": "#ff0055",
        "minimap.warningHighlight": "#ffdd00",
        "minimap.findMatchHighlight": "#0088ff44",
        // === WIDGET ===
        "widget.shadow": "#000000",
        "editorWidget.background": "#0a0a0a",
        "editorWidget.border": "#333333",
        "editorHoverWidget.background": "#0a0a0a",
        "editorHoverWidget.border": "#333333",
        // === PROGRESS BAR ===
        "progressBar.background": "#6366f1",
        // === FOCUS BORDER ===
        "focusBorder": "#00000000",
        // === CONTRAST BORDERS ===
        "contrastBorder": "#333333",
        "contrastActiveBorder": "#00000000",
        // === EXPLORER SPECIFIC ===
        "tree.indentGuidesStroke": "#6366f133",
        "tree.inactiveIndentGuidesStroke": "#6366f133"
    },
    "tokenColors": [
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#4a4a4a"
            }
        },
        {
            "name": "Keyword, Storage - Armonía con verde",
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier",
                "keyword.control",
                "keyword.operator.new",
                "keyword.other.special-method",
                "keyword.control.import",
                "keyword.control.export",
                "keyword.control.default",
                "keyword.control.go"
            ],
            "settings": {
                "foreground": "#66ccff",
                "fontStyle": ""
            }
        },
        {
            "name": "String - Sutil pero visible",
            "scope": [
                "string",
                "string.quoted",
                "string.template",
                "punctuation.definition.string"
            ],
            "settings": {
                "foreground": "#99ddaa"
            }
        },
        {
            "name": "Number",
            "scope": [
                "constant.numeric",
                "constant.language",
                "constant.character",
                "constant.escape"
            ],
            "settings": {
                "foreground": "#c4b078"
            }
        },
        {
            "name": "Function - Verde NEON",
            "scope": [
                "entity.name.function",
                "meta.function-call",
                "variable.function",
                "support.function",
                "entity.name.method"
            ],
            "settings": {
                "foreground": "#6366f1",
                "fontStyle": ""
            }
        },
        {
            "name": "Class, Type",
            "scope": [
                "entity.name.class",
                "entity.name.type",
                "support.type",
                "support.class",
                "entity.name.interface"
            ],
            "settings": {
                "foreground": "#7cb3b3"
            }
        },
        {
            "name": "Variable",
            "scope": [
                "variable",
                "variable.parameter",
                "variable.other",
                "variable.language.this",
                "variable.language.super"
            ],
            "settings": {
                "foreground": "#e8e8e8"
            }
        },
        {
            "name": "Property - Más visible para Go",
            "scope": [
                "variable.object.property",
                "meta.object-literal.key",
                "support.variable.property",
                "entity.other.attribute-name.class.css",
                "entity.other.attribute-name.id.css",
                "meta.field.declaration.go",
                "variable.field.go"
            ],
            "settings": {
                "foreground": "#a8d0ff"
            }
        },
        {
            "name": "Tag",
            "scope": [
                "entity.name.tag",
                "punctuation.definition.tag"
            ],
            "settings": {
                "foreground": "#7aa3c4"
            }
        },
        {
            "name": "Attribute",
            "scope": [
                "entity.other.attribute-name",
                "entity.other.attribute-name.html"
            ],
            "settings": {
                "foreground": "#c4b078"
            }
        },
        {
            "name": "Operator - Coherente con verde neón",
            "scope": [
                "keyword.operator",
                "keyword.operator.assignment",
                "keyword.operator.arithmetic",
                "keyword.operator.logical",
                "keyword.operator.comparison.go",
                "punctuation.separator.key-value"
            ],
            "settings": {
                "foreground": "#aaffaa"
            }
        },
        {
            "name": "Punctuation",
            "scope": [
                "punctuation",
                "punctuation.separator",
                "punctuation.terminator"
            ],
            "settings": {
                "foreground": "#707070"
            }
        },
        {
            "name": "Constants",
            "scope": [
                "constant",
                "constant.other",
                "support.constant"
            ],
            "settings": {
                "foreground": "#b078a8"
            }
        },
        {
            "name": "Regular Expression - Verde NEON",
            "scope": [
                "string.regexp",
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#6366f1"
            }
        },
        {
            "name": "Import/Module",
            "scope": [
                "entity.name.import",
                "entity.name.module",
                "support.module"
            ],
            "settings": {
                "foreground": "#6b9bd1"
            }
        },
        {
            "name": "Decorator",
            "scope": [
                "entity.name.function.decorator",
                "meta.decorator",
                "punctuation.decorator"
            ],
            "settings": {
                "foreground": "#a87cb3"
            }
        },
        {
            "name": "Escape Characters - Verde NEON",
            "scope": [
                "constant.character.escape",
                "constant.character.entity"
            ],
            "settings": {
                "foreground": "#6366f1"
            }
        },
        {
            "name": "Template Expression - Verde NEON",
            "scope": [
                "meta.template.expression",
                "punctuation.definition.template-expression"
            ],
            "settings": {
                "foreground": "#6366f1"
            }
        },
        {
            "name": "Go - Package y struct fields más vivos",
            "scope": [
                "entity.name.package.go",
                "variable.other.field.go",
                "entity.name.type.struct.go",
                "meta.struct.go variable.other.go"
            ],
            "settings": {
                "foreground": "#88ddff"
            }
        },
        {
            "name": "Go - Métodos receiver más visible",
            "scope": [
                "meta.function.receiver.go variable.parameter.go",
                "variable.parameter.receiver.go"
            ],
            "settings": {
                "foreground": "#ffaa55"
            }
        },
        {
            "name": "Go - Interfaces y tipos",
            "scope": [
                "entity.name.type.interface.go",
                "support.type.go"
            ],
            "settings": {
                "foreground": "#dd88ff"
            }
        },
        {
            "name": "CSS Property",
            "scope": [
                "support.type.property-name.css"
            ],
            "settings": {
                "foreground": "#7cb3b3"
            }
        },
        {
            "name": "CSS Value",
            "scope": [
                "support.constant.property-value.css",
                "meta.property-value.css"
            ],
            "settings": {
                "foreground": "#c4b078"
            }
        },
        {
            "name": "Markdown Heading - Verde NEON",
            "scope": [
                "markup.heading",
                "entity.name.section.markdown"
            ],
            "settings": {
                "foreground": "#6366f1",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown Bold",
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Go - Error handling más visible",
            "scope": [
                "variable.language.go",
                "constant.language.go"
            ],
            "settings": {
                "foreground": "#ffcc77"
            }
        },
        {
            "name": "Go - Punteros y referencias",
            "scope": [
                "keyword.operator.address.go",
                "keyword.operator.channel.go"
            ],
            "settings": {
                "foreground": "#6366f1",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown Italic",
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#d5d5d5"
            }
        },
        {
            "name": "Markdown Code",
            "scope": [
                "markup.inline.raw",
                "markup.fenced_code"
            ],
            "settings": {
                "foreground": "#8ba888"
            }
        },
        {
            "name": "JSON Key",
            "scope": [
                "support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#7cb3b3"
            }
        },
        {
            "name": "Invalid",
            "scope": [
                "invalid",
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "#ff4757",
                "fontStyle": "bold"
            }
        }
    ]
}
